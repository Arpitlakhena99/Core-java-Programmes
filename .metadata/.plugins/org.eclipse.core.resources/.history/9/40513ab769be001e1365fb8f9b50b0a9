package strings;

public class string20 {
	public static void main(String []args)
	{
		String s1="7f4d5hsre";
		boolean b=true;
		for(int i=0;i<s1.length()-2;i++)
		{
			char  cc=s1.charAt(i);
			if(s1.charAt(i+1)==cc+1&&s1.charAt(i+2)==cc+2)
			{
				b=false;
				
			}
		}
		System.out.println(b);
	}

}
